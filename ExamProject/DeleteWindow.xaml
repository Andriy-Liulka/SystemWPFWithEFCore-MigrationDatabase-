<Window x:Class="ExamProject.DeleteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExamProject"
        mc:Ignorable="d"
        Title="DeleteWindow" Height="450" Width="800"
        Loaded="Window_Loaded">
    <Grid>
        <ComboBox x:Name="ComboBox_WithBooks" FontSize="25" HorizontalAlignment="Left" Margin="222,100,0,0" VerticalAlignment="Top" Width="296" Height="41"></ComboBox>
        <TextBlock HorizontalAlignment="Left" FontSize="25" Margin="232,47,0,0" Text="Choose book to delete:" TextWrapping="Wrap" VerticalAlignment="Top" Height="35" Width="300"/>
        <Button Content="Cancel" FontSize="25" HorizontalAlignment="Left" Margin="259,315,0,0" VerticalAlignment="Top" Height="52" Width="246" Click="Button_Click"/>
        <Button Content="Delete" FontSize="25" HorizontalAlignment="Left" Margin="259,0,0,0" VerticalAlignment="Center" Height="52" Width="246" Click="Delete_Button_Click"/>

    </Grid>
</Window>
